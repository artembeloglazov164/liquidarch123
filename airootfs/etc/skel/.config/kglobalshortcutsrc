[kwin]
Expose=Ctrl+Up,Ctrl+F9,Toggle Present Windows (Current desktop)
ExposeAll=Ctrl+F10\tLaunch (C),Ctrl+F10\tLaunch (C),Toggle Present Windows (All desktops)
ExposeClass=Ctrl+F7,Ctrl+F7,Toggle Present Windows (Window class)
Switch One Desktop Down=none,Meta+Ctrl+Down,Switch One Desktop Down
Switch One Desktop Up=none,Meta+Ctrl+Up,Switch One Desktop Up
Switch One Desktop to the Left=Ctrl+Left,Meta+Ctrl+Left,Switch One Desktop to the Left
Switch One Desktop to the Right=Ctrl+Right,Meta+Ctrl+Right,Switch One Desktop to the Right
Switch Window Down=none,Meta+Alt+Down,Switch to Window Below
Switch Window Left=none,Meta+Alt+Left,Switch to Window to the Left
Switch Window Right=none,Meta+Alt+Right,Switch to Window to the Right
Switch Window Up=none,Meta+Alt+Up,Switch to Window Above
Walk Through Windows=Meta+Tab,Alt+Tab,Walk Through Windows
Walk Through Windows (Reverse)=Meta+Shift+Tab,Alt+Shift+Backtab,Walk Through Windows (Reverse)
Window Close=Meta+Q,Alt+F4,Close Window
Window Maximize=Meta+PgUp,Meta+PgUp,Maximize Window
Window Minimize=Meta+M,Meta+PgDown,Minimize Window

[plasmashell]
activate task manager entry 1=none,Meta+1,Activate Task Manager Entry 1
activate task manager entry 2=none,Meta+2,Activate Task Manager Entry 2
activate task manager entry 3=none,Meta+3,Activate Task Manager Entry 3
activate task manager entry 4=none,Meta+4,Activate Task Manager Entry 4
activate task manager entry 5=none,Meta+5,Activate Task Manager Entry 5
activate widget 3=Meta+Space,none,Activate Application Launcher Widget
show dashboard=none,Ctrl+F12,Show Desktop

[org.kde.dolphin.desktop]
_launch=Meta+E,none,Dolphin

[org.kde.konsole.desktop]
_launch=Meta+T,none,Konsole
